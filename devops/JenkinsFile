pipeline
{
    agent any

    stages
    {
            stage('Construir')
            {
                steps
                {
                    echo 'Construindo..'
                    buildInfo = Artifactory.newMavenBuild().run pom: 'pom.xml', goals: 'clean install'
                }
            }

            stage('Teste')
            {
                steps
                {
                    echo 'Testando..'
                }
            }


            stage('Implantar')
            {
                steps
                {
                    echo 'Implantando....'
                }
            }
    }
}
